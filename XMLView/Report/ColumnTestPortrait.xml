            <column posx="200" width="20">
                <widget type="label" value="-" />
            </column>
            <column posx="300" width="20">
                <widget type="label" value="-" />
            </column>
            <column posx="400" width="20">
                <widget type="label" value="-" />
            </column>
            <column posx="500" width="20">
                <widget type="label" value="-" />
            </column>
            <column posx="550" width="20">
                <widget type="label" value="-" />
            </column>
            <column posx="560" width="20">
                <widget type="label" fieldname="id" />
            </column>
