<report>
    <config>
        <page type="A4" orientation="portrait" />
        <font type="Arial" size="12" />
        <default group="main" alter="other" />
    </config>

    <group name="main">
        <header height="40">
            <column posx="10" posy="15" width="20">
                <widget type="label" value="5" />
            </column>
            <column posx="25" posy="15" width="20">
                <widget type="label" value="25" />
            </column>
            <column posx="50" posy="15" width="20">
                <widget type="label" value="50" />
