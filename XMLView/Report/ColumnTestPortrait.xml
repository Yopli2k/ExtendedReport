            <column posx="200" posy="15" width="20">
                <widget type="label" value="200" />
            </column>
            <column posx="300" posy="15" width="20">
                <widget type="label" value="300" />
            </column>
            <column posx="400" posy="15" width="20">
                <widget type="label" value="400" />
            </column>
            <column posx="500" posy="15" width="20">
